{
	"conda": {
		"sysreqs": "/conda\\s*\\(([^)]+)\\)/i",
		"satisfies": "python-2.7",
		"platforms": {
			"Windows": "conda.exe",
			"DEB": "conda",
			"RPM": "conda"
		},
		"handlers": {
		  "install": "[skip]",
			"check": "[tool]",
			"build": "[shell] :tool create -y -p :path :params"
		}
	}
}
